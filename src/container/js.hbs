<div>
  <h1>{{name}}</h1>
  <p>{{description}}</p>
</div>
<hr />
<br />
<div>
  {{#with history}}
    <li>{{year}}</li>
    <li>{{founder}}</li>
  {{/with}}
</div>
<hr />
<br />
<div>
  {{#each types}}
    <li>{{this}}</li>
  {{/each}}
</div>
<hr />

<div>
  <h3>Sintax</h3>
  {{#with syntax}}
    <ul>
      <b>Virables:</b>
      {{#each variables}}
        <li>{{this}}</li>
      {{/each}}
      <br />
      <b>Functions:</b>
      {{#each functions}}
        <li>{{this}}</li>
      {{/each}}
      <br />
      <b>Conditionals:</b>
      {{#each conditionals}}
        <li>{{this}}</li>
      {{/each}}
      <br />
      <b>Loops:</b>
      {{#each loops}}
        <li>{{this}}</li>
      {{/each}}
      <br />
      <b>Classes:</b>
      {{#each classes}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
  {{/with}}
</div>
<hr />
<br />
<div>
  <b>Features:</b>
  {{#each features}}
    <li>{{this}}</li>
  {{/each}}
</div>
<hr />
<div>
  <b>Libraries</b>
  {{#each libraries}}
    <li>{{this}}</li>
  {{/each}}
</div>
<hr />
<div>
  <b>Tools:</b>
  {{#each tools}}
    <li>{{this}}</li>
  {{/each}}
</div>
<hr />
<div>
  <b>Community:</b>
  {{#each community}}
    <ul>
      <li>{{name}}</li>
      <li>{{type}}</li>
    </ul>
  {{/each}}
</div>