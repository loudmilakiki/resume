<div>
  {{#with page_header}}
    <h1 class='text-center'>{{title}}</h1>
    <p
      class='text-center text-uppercase'
    >{{description}}</p>
  {{/with}}
</div>
<br />
<ul>
  <div>
    {{#with section}}
      <div class='card'>
        <div class='card-body'>
          {{#with about_us}}
            <div>
              {{#with title_list}}
                <div>
                  <div
                    class='display-6 text-end'
                  >{{main}}</div>
                  <h2 class='text-end'>{{sub}}</h2>
                </div>
              {{/with}}
              <br />
              {{#with button}}
                <button
                  class='btn btn-outline-info'
                >{{text}}</button>
              {{/with}}
            </div>
          {{/with}}
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with advantage}}
            <div class='display-5'>{{title}}</div>
            <br />
            <div>
              <div class='card'>
                <div class='card-body'>
                  {{#with efficiency}}
                    <div>
                      <div class='display-4'>{{title}}</div>
                      <br />
                      <h6>{{sub_title}}</h6>
                      <br />
                      <p>{{description}}</p>
                      <br />
                      <button
                        class='btn btn-outline-warning'
                      >{{button}}</button>
                    </div>
                  {{/with}}
                </div>
              </div>
            </div>
            <br />
            <div>
              <div class='card'>
                <div class='card-body'>
                  {{#with innovation}}
                    <div
                      class='display-4 text-center'
                    >{{title}}</div>
                    <br />
                    <h6
                      class='text-center'
                    >{{sub_title}}</h6>
                    <br />
                    <p
                      class='text-center'
                    >{{description}}</p>
                    <button
                      class='btn btn-outline-warning'
                    >{{button}}</button>
                  {{/with}}
                </div>
              </div>
            </div>
            <br />
            <div>
              <div class='card'>
                <div class='card-body'>
                  {{#with collaboration}}
                    <div
                      class='display-4 text-end'
                    >{{title}}</div>
                    <br />
                    <h6 class='text-end'>{{sub_title}}</h6>
                    <br />
                    <p class='text-end'>{{description}}</p>
                    <button
                      class='btn btn-outline-warning'
                    >{{button}}</button>
                  {{/with}}
                </div>
              </div>
            </div>
          {{/with}}
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          {{#with agreement}}
            <div class='display-5'>{{title}}</div>
            <br />
            <div class='card'>
              <div class='card-body'>
                {{#with info}}
                  <h5>{{sub_title}}</h5>
                  <br>
                  <div class='card'>
                    <div class='card-body'>
                      {{#with detail}}
                      <br>
                        <p>{{text}}</p>
                        <br>
                        <button
                          class='btn btn-success'
                        >{{button}}</button>
                      {{/with}}
                    </div>
                  </div>
                {{/with}}
              </div>
            </div>
          {{/with}}           
    {{/with}}
  </div>
</ul>