<div>
  {{#with navigation}}
    <div class='card pt-3 pb-2 px-2 mb-4'>
      <nav class='row'>

        <div class='col-6'>
          <h1 class='m-3'>{{Logo}}</h1>
        </div>

        <div class='col-6 p-0 text-end'>
          {{#each links}}
            {{#if href}}
              <a
                href='{{href}}'
                class='btn btn-link m-3'
              >{{text}}</a>{{/if}}

          {{/each}}
          <a
            href='#'
            class='btn btn-link disabled'
          >Disabled</a>
        </div>

      </nav>
    </div>
  {{/with}}
</div>